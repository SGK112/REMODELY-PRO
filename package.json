{
  "name": "remodely-ai-marketplace",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "dev": "next dev -p 3001",
    "dev:clean": "rm -rf .next && npm run dev",
    "dev:turbo": "next dev --turbo -p 3001",
    "build": "npm install --legacy-peer-deps && next build",
    "build:analyze": "ANALYZE=true next build",
    "build:optimized": "./scripts/optimize-build.sh",
    "build:production": "npm run db:generate && NODE_OPTIONS='--max-old-space-size=4096' npm install --legacy-peer-deps && next build",
    "start": "next start -p ${PORT:-10000}",
    "start:production": "NODE_ENV=production npm start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "clean": "rm -rf .next out node_modules/.cache",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "deploy:optimized": "npm run build:optimized && npm run start:production",
    "health-check": "curl -f http://localhost:${PORT:-3001}/api/health || exit 1",
    "test:core": "jest --testPathPattern=core",
    "test:integration": "node test-scripts/run-integration-tests.js"
  },
  "dependencies": {
    "@11labs/react": "^0.1.4",
    "@elevenlabs/client": "^0.4.4",
    "@elevenlabs/react": "^0.4.5",
    "@google-cloud/aiplatform": "^5.1.0",
    "@google-cloud/vision": "^5.3.2",
    "@google/genai": "^1.12.0",
    "@google/generative-ai": "^0.24.1",
    "@googlemaps/js-api-loader": "^1.16.10",
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^3.3.2",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^5.22.0",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.1.2",
    "@radix-ui/react-slot": "^1.2.3",
    "@shopify/shopify-api": "^11.14.1",
    "@stripe/stripe-js": "^2.2.0",
    "@tailwindcss/forms": "^0.5.10",
    "@types/bcryptjs": "^2.4.6",
    "@types/cheerio": "^0.22.35",
